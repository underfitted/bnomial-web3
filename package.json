{
    "name": "Underfitted-Membership-NFT",
    "version": "0.1.0",
    "description": "Smart contract for the Underfitted Membership NFT token",
    "scripts": {
        "build": "hardhat compile",
        "test": "hardhat test",
        "gas": "REPORT_GAS=1 hardhat test",
        "coverage": "hardhat coverage",
        "deploy:polygon": "npx hardhat run scripts/deploy.js --network polygon",
        "deploy:mumbai": "npx hardhat run scripts/deploy.js --network mumbai",
        "verify:polygon": "npx hardhat verify --network polygon",
        "verify:mumbai": "npx hardhat verify --network mumbai",
        "lint": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
        "slighter": "slither . --ignore-compile --filter-paths \"node_modules\" --disable-color --print human-summary > slither-audit-3.txt 2>&1"
    },
    "author": "Underfitted Social Club",
    "license": "MIT",
    "devDependencies": {
        "@nomiclabs/hardhat-ethers": "^2.0.3",
        "@nomiclabs/hardhat-etherscan": "^2.1.8",
        "@nomiclabs/hardhat-waffle": "^2.0.1",
        "@openzeppelin/contracts": "^4.4.1",
        "chai": "^4.3.4",
        "dotenv": "^10.0.0",
        "ethereum-waffle": "^3.4.0",
        "ethers": "^5.5.1",
        "hardhat": "^2.8.0",
        "hardhat-gas-reporter": "^1.0.6",
        "prettier": "2.5.1",
        "solhint": "^3.3.6",
        "solidity-coverage": "^0.7.17",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "ci": "^2.1.1"
    }
}
