{
    "name": "underfitted-membership-nft",
    "version": "0.1.0",
    "description": "Smart contract for the Underfitted Membership NFT token",
    "scripts": {
        "build": "hardhat compile",
        "test": "hardhat test",
        "gas": "REPORT_GAS=1 hardhat test",
        "coverage": "hardhat coverage",
        "deployNFT:polygon": "npx hardhat run scripts/deployNFT.js --network polygon",
        "deployNFT:mumbai": "npx hardhat run scripts/deployNFT.js --network mumbai",
        "deployToken:polygon": "npx hardhat run scripts/deployToken.js --network polygon",
        "deployToken:mumbai": "npx hardhat run scripts/deployToken.js --network mumbai",
        "verify:polygon": "npx hardhat verify --network polygon",
        "verify:mumbai": "npx hardhat verify --network mumbai"
    },
    "author": "Underfitted Social Club",
    "license": "MIT",
    "devDependencies": {
        "@nomiclabs/hardhat-ethers": "^2.0.2",
        "@nomiclabs/hardhat-etherscan": "^2.1.7",
        "@nomiclabs/hardhat-waffle": "^2.0.1",
        "chai": "^4.3.4",
        "ethereum-waffle": "^3.4.0",
        "ethers": "^5.5.1",
        "hardhat": "^2.6.8",
        "hardhat-gas-reporter": "^1.0.4",
        "solidity-coverage": "^0.7.17"
    },
    "dependencies": {
        "@openzeppelin/contracts": "^4.3.3",
        "dotenv": "^10.0.0"
    }
}
